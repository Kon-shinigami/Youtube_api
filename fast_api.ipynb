    "from fastapi import FastAPI\n",
    "import json\n",
    "\n",
    "app = FastAPI()\n",
    "\n",
    "with open(\"answers.json\") as ans:\n",
    "    answers = json.load(ans)\n",
    "\n",
    "@app.get(\"/answers\")\n",
    "def get_answers():\n",
    "    return answers\n"
   ]
